/**
* Make cretit card default for current customer
*
* @input cardId : String
*/
importPackage( dw.system );

/* Stripe include */
var Stripe = require('int_stripe/cartridge/scripts/service/stripe');

function execute( pdict : PipelineDictionary ) : Number
{
	var cartId : String = args.cardId;
	Stripe.MakeDefault(cardId);

    return PIPELET_NEXT;
}
